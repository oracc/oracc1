003. %a |MIN.MIN|<({d}ša₂-maš)>
     {d}ša₂-maš
     <g:w form="{d}ša₂-maš" xml:lang="akk">
       <g:surro>
         <g:c form="|MIN.MIN|">
           <g:s>MIN</g:s>
           <g:o g:type="beside"></g:o>
           <g:s>MIN</g:s>
         </g:c>
         <g:gg g:type="group">
           <g:d g:pos="pre" g:role="semantic">
             <g:v>d</g:v>
            </g:d>
           <g:v g:delim="-">ša₂</g:v>
           <g:v g:surroEnd="X000001.1.1.0">maš</g:v>
         </g:gg>
       </g:surro>

004. %a DAM#{+mu-us-sa}-BI
     DAM{+mu-us-sa}-BI
     <g:w form="DAM{+mu-us-sa}-BI" xml:lang="akk">
       <g:gg g:type="logo">
         <g:s g:break="damaged" g:hc="1" g:ho="1" g:logolang="sux" g:role="logo">DAM</g:s>
         <g:d g:delim="-" g:pos="post" g:role="phonetic">
           <g:v g:delim="-">mu</g:v>
           <g:v g:delim="-">us</g:v>
           <g:v>sa</g:v>
         </g:d>
         <g:s g:logolang="sux" g:role="logo">BI</g:s>
       </g:gg>
     </g:w>

005. %a SUH.[...]{MIN<(...)>}
     SUH.x{x}
     <g:w form="SUH.x{x}" xml:lang="akk">
       <g:gg g:type="logo">
         <g:s g:delim="." g:logolang="sux" g:role="logo">SUH</g:s>
         <g:x g:break="missing" g:breakEnd="X000001.1.1.1" g:breakStart="1" g:c="]" g:o="[" g:type="ellipsis">...</g:x>
         <g:d g:pos="post" g:role="semantic">
           <g:surro>
             <g:s>MIN</g:s>
             <g:gg g:type="group">
               <g:x g:type="ellipsis">...</g:x>
             </g:gg>
           </g:surro>
         </g:d>
       </g:gg>
     </g:w>

